{
	"name": "google-drive-vscode",
	"displayName": "Google Drive",
	"description": "Google Drive support for VSCode",
	"author": {
		"name": "Gustavo Cassel",
		"email": "guscassel@gmail.com"
	},
	"publisher": "GustavoASC",
	"version": "0.0.2",
	"icon": "img/icon.png",
	"engines": {
		"vscode": "^1.43.0"
	},
	"bugs": {
		"url": "https://github.com/GustavoASC/google-drive-vscode/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/GustavoASC/google-drive-vscode.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:google.drive.configureCredentials",
		"onCommand:google.drive.fetchFiles",
		"onCommand:google.drive.uploadOpenFile",
		"onCommand:google.drive.uploadSelectedFile",
		"onCommand:google.drive.createFolder",
		"onView:driveView"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "google.drive.configureCredentials",
				"title": "Google Drive: Configure credentials"
			},
			{
				"command": "google.drive.fetchFiles",
				"title": "Google Drive: Fetch files from Drive",
				"icon": "$(search-refresh)"
			},
			{
				"command": "google.drive.uploadOpenFile",
				"title": "Google Drive: Upload currently open file to Drive"
			},
			{
				"command": "google.drive.uploadSelectedFile",
				"title": "Google Drive: Upload selected file to Drive"
			},
			{
				"command": "google.drive.createFolder",
				"title": "Google Drive: Create folder on Drive",
				"icon": "$(new-folder)",
				"enablement": "viewItem != file"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "driveActivityBar",
					"title": "Google Drive",
					"icon": "img/icon.png"
				}
			]
		},
		"views": {
			"driveActivityBar": [
				{
					"id": "driveView",
					"name": "Google Drive"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "google.drive.createFolder",
					"when": "view == driveView",
					"group": "navigation"
				},
				{
					"command": "google.drive.fetchFiles",
					"when": "view == driveView",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "google.drive.createFolder",
					"when": "view == driveView",
					"group": "inline"
				}
			],
			"explorer/context": [
				{
					"command": "google.drive.uploadSelectedFile",
					"group": "drive"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npm run tslint && tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"tslint": "tslint --project tsconfig.json --config tslint.json"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/mime-types": "^2.1.0",
		"tslint": "^5.14.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode": "1.1.36",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"googleapis": "52.1.0",
		"keytar": "6.0.1",
		"mime-types": "2.1.27"
	},
	"__metadata": {
		"id": "d4e69f94-ffae-47b6-a315-e5f90289a7c2",
		"publisherDisplayName": "Gustavo Cassel",
		"publisherId": "6290af19-eb5e-4f57-8ff3-c76e661f7bc2"
	}
}